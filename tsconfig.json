{
    "compilerOptions": {
      "typeRoots": ["./node_modules/@types"],
      "types": ["node"],
      "maxNodeModuleJsDepth": 2,
      "skipLibCheck": true,
      "disableSourceOfProjectReferenceRedirect": true,
      "useUnknownInCatchVariables": false,
      "paths": {
        "*": [
          "src/types/*",
          "*"
        ]
      },
      "allowJs": true
    },
    "incremental": true
  }
  